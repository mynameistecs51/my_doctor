<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".page_1">

    <TextView
        android:id="@+id/namep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="98dp"
        android:text="ชื่อผู้ป่วย : "
        android:textSize="18sp" />

    <EditText
        android:id="@+id/pname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/namep"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="-8dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/agep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="139dp"
        android:text="อายุ :"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/page"
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/pname"
        android:layout_alignBottom="@+id/agep"
        android:layout_marginEnd="182dp"
        android:layout_marginBottom="-5dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/sexp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/agep"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="206dp"
        android:layout_marginBottom="-1dp"
        android:text="เพศ :"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/Psex"
        android:layout_width="86dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/agep"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="149dp"
        android:layout_marginBottom="345dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/dateP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="180dp"
        android:text="วันที่เริ่มทำการรักษา :"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/Pdate"
        android:layout_width="147dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/Psex"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-40dp"
        android:layout_marginBottom="295dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/congenita"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/namep"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="269dp"
        android:text="โรคประจำตัว :"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/Pcongenita"
        android:layout_width="345dp"
        android:layout_height="83dp"
        android:layout_alignStart="@+id/agep"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-2dp"
        android:layout_marginBottom="180dp"
        android:ems="10"
        android:inputType="textMultiLine" />

    <TextView
        android:id="@+id/adminP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/namep"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-14dp"
        android:layout_marginBottom="115dp"
        android:text="ผู้ดูแล :"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/Aboutadmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/adminP"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="-2dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/namep"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-12dp"
        android:layout_marginBottom="76dp"
        android:text="เกี่ยวข้องเป็น :"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/adminabout"
        android:layout_width="167dp"
        android:layout_height="35dp"
        android:layout_alignEnd="@+id/Pcongenita"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="122dp"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="69dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="99dp"
        android:text="ข้อมูลส่วนตัวผู้ป่วย"
        android:textSize="24sp" />
</RelativeLayout>